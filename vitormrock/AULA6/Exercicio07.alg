Algoritmo "EXERCICIO 07"

 var
   i, n: inteiro
inicio
   escreva("Informe o valor de N: ")
   leia(n)
   
   enquanto (n > 0) faca
      para i de i ate n faca
         escreval(i, " x ", n, " = ", i * n)
      fimpara
      escreva("Informe o valor de N: ")
      leia(n)
   fimenquanto
fimalgoritmo

