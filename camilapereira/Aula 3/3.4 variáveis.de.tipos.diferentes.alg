algoritmo "endereco"
var
   rua: caractere
   numero: inteiro
   area: real
   apartamento: logico
inicio
      Escreval ("digite o a sua rua:")
      Leia (rua)
      Escreval ("digite o numero da sua casa:")
      Leia (numero)
      Escreval ("digite a area da sua casa:")
      Leia (area)
      Escreval ("você mora em um apartamento?")
      Leia (apartamento)
fimalgoritmo
