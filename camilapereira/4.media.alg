algoritmo "media"

var
   numero1, numero2: inteiro
   media: real
   inicio
      escreval("Digite o primeiro número inteiro: ")
      leia(numero1)
      escreval("Digite o segundo número inteiro: ")
      leia(numero2)
      media <- (numero1 + numero2) / 2
      escreva (media)
   fimalgoritmo
