algoritmo "endereco"
var
   nome: caractere
   telefone: caractere
   endereco: caractere
   idade: inteiro
inicio
      Escreval ("digite o seu nome:")
      Leia (nome)
      Escreval ("digite o seu telefone:")
      Leia (telefone)
      Escreval ("digite o seu endereço:")
      Leia (endereco)
      Escreval ("digite a sua idade:")
fimalgoritmo
