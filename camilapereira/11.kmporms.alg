algoritmo "km/m por ms/"

   var
      quilometrosporhora, metrosporsegundo: real

   Inicio
      escreval ("Informe a velocidade do veiculo em quilômetros por hora ou km/h: ")
      leia(quilometrosporhora)
      metrosporsegundo <- quilometrosporhora /3,6
      escreval ("Resultado em m/s: ", metrosporsegundo)
fimalgoritmo

