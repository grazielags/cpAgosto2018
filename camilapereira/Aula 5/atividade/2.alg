algoritmo "valor disponivel"
var
   valor1 : inteiro
   valor2 : inteiro
inicio
   escreval("digite o primeiro valor:")
   leia(valor1)
   escreval("digite o segundo valor:")
   leia(valor2)
   se valor1 > valor2 entao
      Escreval("O valor ", valor1, " é maior que ", valor2)
   senao
      Escreval("O valor ", valor2, " é maior que", valor1)
   fimse
fimalgoritmo
