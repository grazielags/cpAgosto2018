algoritmo "valor disponivel"
var
   n1 : inteiro
   n2 : inteiro
inicio
   escreval("digite o primeiro numero:")
   leia(n1)
   escreval("digite o segundo numero:")
   leia(n2)
   se (n1 > n2) entao
      Escreval("diferença:", (n1 - n2))
   senao
      Escreval("diferença:", (n2 - n1))
   fimse
fimalgoritmo
