algoritmo "2 fatorial"
var
   n, fatorial: inteiro
inicio
   escreval("digite o numero para descobrir o fatorial")
   leia(n)
   fatorial <- n
   enquanto (n > 1) faca
            fatorial <- fatorial * (n - 1)
            n <- n - 1
   fimenquanto
   escreval ("O fatorial é: ", fatorial)
fimalgoritmo

