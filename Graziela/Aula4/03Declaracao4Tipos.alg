algoritmo "Declaracao4Tipos"
var
   idade : inteiro
   nome : caractere
   salario : real
   isFeminino : logico
inicio
   idade <- 20
   nome <- "Graziela"
   salario <- 20.0
   isFeminino <- verdadeiro
   Escreval("Idade: ", idade)
   Escreval("Nome: ", nome)
   Escreval("Salário: ", salario)
   Escreval("É feminino? ", isFeminino)
fimalgoritmo
