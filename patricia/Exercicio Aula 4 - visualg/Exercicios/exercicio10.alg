algoritmo "exercicio7"

var
   A,B,C: inteiro
inicio
      Escreval ("Digite valor para A:")
      leia (A)
      Escreval ("Digite valor para B:")
      leia (B)
      C <-A
      A <-B
      B<-C
      Escreval ("O novo valor de A =", A)
      Escreval ("O novo valor de B =", B)
fimalgoritmo
