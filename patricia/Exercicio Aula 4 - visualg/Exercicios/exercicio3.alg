algoritmo "ExerciciosVisualG"
var
   nome: caractere
   idade: inteiro
   casada: logico
   altura: real
inicio
      Escreval ("Qual seu nome?")
      Leia (nome)
      Escreval ("Qual sua idade?")
      Leia (idade)
      Escreval ("Voce e casado?")
      Leia (casada)
      Escreval ("Qual sua altura?")
      Leia (altura)
fimalgoritmo
