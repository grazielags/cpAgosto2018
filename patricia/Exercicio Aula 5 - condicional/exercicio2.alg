algoritmo "condicionalsimples"

var
   valor1, valor2: inteiro
inicio
      Escreval ("Valor 1")
      leia (valor1)
      Escreval ("Valor 2")
      leia (valor2)
      se valor1 > valor2 entao
         escreval("O valor 1 e maior que o valor 2")
      senao
         escreval ("O valor 2 e maior que o valor 1")
      fimse
fimalgoritmo
