algoritmo "condicionalsimples"

var
   valor1, valor2: inteiro
inicio
      Escreval ("Valor 1")
      leia (valor1)
      Escreval ("Valor 2")
      leia (valor2)
      se valor1 > valor2 entao
         escreval("O valor", valor1, " e maior que o valor", valor2)
      senao
         escreval ("O valor", valor2, " e maior que o valor", valor1)
      fimse
fimalgoritmo
