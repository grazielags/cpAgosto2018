algoritmo "tabuada"

var
num, count, resposta:inteiro
inicio

count <- 0
escreval ("digite numero:")
leia (num)
     enquanto count <=10 faca
              resposta<- num*count
              escreval (num,"x",count,"=",resposta)
              count  <- count + 1
              fimenquanto

fimalgoritmo
