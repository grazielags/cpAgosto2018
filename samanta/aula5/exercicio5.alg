algoritmo "exercicio5"

var
  num1:inteiro
inicio
escreval (" digite num1: ")
  leia (num1)
  se (num1 mod 2) = 0  entao
  escreval (num1, " é par")
  senao
  escreval ("é impar")
  fimse
  se (num1 > 0)entao
  escreval ("é positivo")
  senao
  escreval ("é negativo")
  fimse
fimalgoritmo
