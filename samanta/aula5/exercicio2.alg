algoritmo "exercicio2"

var
  valora, valorb: inteiro
  
  
inicio
    escreval ("digite valora: ")
    leia (valora)
    escreval ("digite valorb: ")
    leia (valorb)
    se valora>valorb entao
    escreval ("valora é maior do que valorb")
    senao
    escreval ("valorb é maior do que valora ")
    fimse


fimalgoritmo
