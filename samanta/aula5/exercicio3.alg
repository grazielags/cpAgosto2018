algoritmo "exercicio3"

var
num1, num2:inteiro


inicio

escreval ("digite num1: ")
leia (num1)
escreval ("digite num2: ")
leia (num2)
se (num1>num2)entao
     escreval ("diferença: ", (num1 - num2))
senao
      escreval ("diferença: ", (num2 - num1))
      fimse
     
fimalgoritmo
