algoritmo "exercicio4"

var

   x,y,aux:inteiro
   resultado: real
inicio


escreval ("digite valor x: ")
leia (x)
escreval ("digite valor y: ")
leia (y)
     resultado <-1
   para aux de 1 ate y faca
      resultado<-resultado*x
   fimpara
      escreval ("valor resultado: ", resultado)
      
fimalgoritmo
