// Seção de Declarações 
var
A, B, troca: inteiro

inicio
// Seção de Comandos
escreval("Informe o valor da variável A: ")
leia(a)
troca <- b
escreval("Informe o valor da variável B: ")
leia(b)
troca <- a
a <- b
b <- troca
escreval("O novo valor de A é: ", a)
escreval("O novo valor de B é: ", b)

fimalgoritmo
