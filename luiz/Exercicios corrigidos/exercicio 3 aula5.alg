algoritmo "resultado da diferença"
var
val1, val2, resultado: inteiro
inicio
escreval("digite primeiro valor:")
leia(val1)
escreval("digite segundo valor:")
leia(val2)
se val1 > val2 entao
   resultado <- valor1 - valor2
senao
   resultado <- val2 - val1
fimse
   Escreva("A diferença dos valores é: ", resultado)
fimalgoritmo
